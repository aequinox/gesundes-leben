{
  "name": "git-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "predev": "pagefind --site dist --output-path ./public/index --exclude-selectors 'details, #toc, #inhaltsverzeichnis'",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "postbuild": "pagefind --site dist  --output-subdir index --exclude-selectors 'details, #toc, #inhaltsverzeichnis'",
    "deploy": "bun run build && git checkout --orphan gh-pages && git --work-tree dist add --all && git --work-tree dist commit -m 'Deploy' && git push github gh-pages --force && git checkout -f main && git branch -D gh-pages",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check . --plugin=prettier-plugin-astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro --plugin prettier-plugin-tailwindcss",
    "lint": "eslint .",
    "lint-staged": "lint-staged --config lint-staged.config.cjs",
    "prepare": "husky",
    "precommit": "lint-staged",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "4.2.3",
    "@astrojs/react": "4.2.3",
    "@astrojs/sitemap": "^3.3.0",
    "@astrojs/tailwind": "6.0.2",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "astro": "5.6.1",
    "astro-pagefind": "^1.8.3",
    "astro-seo-meta": "^5.1.0",
    "fuse.js": "^7.1.0",
    "inquirer": "^12.5.2",
    "inversify": "^7.5.0",
    "luxon": "^3.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.17",
    "turndown": "^7.2.0",
    "turndown-plugin-gfm": "^1.0.2",
    "typescript": "^5.8.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@astrojs/rss": "^4.0.11",
    "@eslint/js": "^9.24.0",
    "@iconify-json/tabler": "^1.2.17",
    "@iconify/tailwind": "^1.2.0",
    "@resvg/resvg-js": "^2.6.2",
    "@tabler/icons-react": "^3.31.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/aos": "^3.0.7",
    "@types/axios": "^0.14.4",
    "@types/chalk": "^2.2.4",
    "@types/he": "^1.2.3",
    "@types/js-yaml": "^4.0.9",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.14.1",
    "@types/turndown": "^5.0.5",
    "@types/uuid": "^10.0.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "aos": "^2.3.4",
    "astro-eslint-parser": "^1.2.2",
    "astro-icon": "^1.1.5",
    "eslint": "^9.24.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "gray-matter": "^4.0.3",
    "hast-util-to-string": "^3.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-to-string": "^4.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-compiler-runtime": "^19.0.0-beta-e993439-20250405",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.12.2",
    "slugify": "^1.6.6",
    "uuid": "^11.1.0",
    "vitest": "^3.1.1",
    "yaml": "^2.7.1"
  },
  "comments": "https://github.com/withastro/astro/issues/12723",
  "resolutions": {
