{
  "name": "git-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "predev": "pagefind --site dist --output-path ./public/index --exclude-selectors 'details, #toc, #inhaltsverzeichnis'",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "postbuild": "pagefind --site dist  --output-subdir index --exclude-selectors 'details, #toc, #inhaltsverzeichnis'",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check . --plugin=prettier-plugin-astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro --plugin prettier-plugin-tailwindcss",
    "lint": "eslint .",
    "lint-staged": "lint-staged --config lint-staged.config.cjs",
    "prepare": "husky",
    "precommit": "lint-staged",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "4.0.3",
    "@astrojs/react": "4.1.2",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "astro": "5.1.3",
    "astro-pagefind": "^1.7.0",
    "fuse.js": "^7.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@astrojs/rss": "4.0.11",
    "@eslint/js": "^9.17.0",
    "@iconify-json/tabler": "^1.2.14",
    "@iconify/tailwind": "^1.2.0",
    "@resvg/resvg-js": "^2.6.2",
    "@tabler/icons-react": "^3.28.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/aos": "^3.0.7",
    "@types/node": "^22.10.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/ui": "^2.1.8",
    "aos": "^2.3.4",
    "astro-eslint-parser": "^1.1.0",
    "astro-icon": "^1.1.5",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "gray-matter": "^4.0.3",
    "hast-util-to-string": "^3.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-to-string": "^4.0.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.11.3",
    "slugify": "^1.6.6",
    "uuid": "^11.0.5",
    "vitest": "^2.1.8",
    "yaml": "^2.7.0"
  }
}
