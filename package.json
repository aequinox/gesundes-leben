{
  "name": "healthy-life",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "check": "NODE_OPTIONS='--max-old-space-size=8192' astro check",
    "dev": "astro dev",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' astro build",
    "build:check": "NODE_OPTIONS='--max-old-space-size=16384' astro check && NODE_OPTIONS='--max-old-space-size=8192' astro build",
    "type-check": "NODE_OPTIONS='--max-old-space-size=16384' tsc --noEmit",
    "postbuild": "pagefind --site dist --exclude-selectors 'details, #toc, #inhaltsverzeichnis' --output-path ./public/pagefind",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --project unit",
    "test:integration": "vitest run --project integration",
    "test:component": "vitest run --project component",
    "test:health": "vitest run --project health",
    "test:unit:watch": "vitest --project unit",
    "test:integration:watch": "vitest --project integration",
    "test:component:watch": "vitest --project component",
    "test:health:watch": "vitest --project health",
    "test:all": "vitest run",
    "test:unit:coverage": "vitest run --project unit --coverage",
    "test:integration:coverage": "vitest run --project integration --coverage",
    "test:component:coverage": "vitest run --project component --coverage",
    "test:health:coverage": "vitest run --project health --coverage",
    "test:coverage:all": "vitest run --coverage",
    "test:coverage:merge": "node scripts/merge-coverage.js",
    "test:coverage:report": "vitest run --coverage && open coverage/index.html",
    "test:coverage:ci": "vitest run --coverage --reporter=json --reporter=github-actions",
    "analyze": "node scripts/analyze-bundle.js",
    "build:analyze": "bun run build && bun run analyze",
    "perf:test": "node tests/performance/run-tests.js",
    "docs:generate": "node scripts/generate-component-docs.js",
    "refs:validate": "bun run scripts/validate-references.ts",
    "refs:validate:analysis": "bun run scripts/validate-references.ts --analysis",
    "refs:validate:errors": "bun run scripts/validate-references.ts --errors-only",
    "refs:manage": "bun run scripts/manage-references.ts",
    "refs:list": "bun run scripts/manage-references.ts list",
    "refs:stats": "bun run scripts/manage-references.ts stats",
    "refs:duplicates": "bun run scripts/manage-references.ts duplicates",
    "refs:export": "bun run scripts/manage-references.ts export",
    "refs:validate:links": "bun run scripts/validate-links.ts",
    "refs:validate:links:verbose": "bun run scripts/validate-links.ts --verbose",
    "refs:validate:links:errors": "bun run scripts/validate-links.ts --errors-only",
    "a11y:test": "node tests/accessibility/a11y-test.js",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen http://localhost:4321",
    "test:e2e:features": "playwright test tests/e2e/features",
    "test:e2e:accessibility": "playwright test tests/e2e/accessibility",
    "test:e2e:performance": "playwright test tests/e2e/performance",
    "test:e2e:integration": "playwright test tests/e2e/integration",
    "test:e2e:static": "playwright test tests/e2e/static-pages",
    "test:e2e:content": "playwright test tests/e2e/content-collections",
    "test:e2e:german": "playwright test tests/e2e/features/german-language-support.spec.ts",
    "test:e2e:health": "playwright test tests/e2e/features/health-content-validation.spec.ts",
    "test:e2e:search": "playwright test tests/e2e/features/search-interactions.spec.ts",
    "test:e2e:navigation": "playwright test tests/e2e/features/navigation.spec.ts",
    "test:e2e:rss": "playwright test tests/e2e/integration/rss-feed.spec.ts",
    "test:e2e:sitemap": "playwright test tests/e2e/integration/sitemap.spec.ts",
    "test:e2e:edge-cases": "playwright test tests/e2e/integration/edge-cases.spec.ts",
    "test:e2e:core-vitals": "playwright test tests/e2e/performance/core-web-vitals.spec.ts",
    "test:e2e:images": "playwright test tests/e2e/performance/image-optimization.spec.ts",
    "test:e2e:wcag": "playwright test tests/e2e/accessibility/wcag-compliance.spec.ts",
    "test:e2e:ci": "playwright test --reporter=github-actions",
    "prepare": "husky",
    "wp-convert": "tsx src/scripts/wp-to-mdx/index.ts convert",
    "xml-convert": "bun scripts/xml-converter.ts",
    "deploy": "bun run build && git checkout --orphan gh-pages && git --work-tree dist add --all && git --work-tree dist commit -m 'Deploy' && git push github gh-pages --force && git checkout -f main && git branch -D gh-pages"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.1",
    "@astrojs/rss": "4.0.12",
    "@astrojs/sitemap": "3.4.1",
    "@iconify-json/tabler": "^1.2.19",
    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/vite": "^4.1.11",
    "@types/chalk": "^2.2.4",
    "astro": "5.12.0",
    "astro-icon": "^1.1.5",
    "astro-pagefind": "^1.8.3",
    "astro-robots-txt": "^1.0.0",
    "axios": "^1.10.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dayjs": "^1.11.13",
    "hast-util-to-string": "^3.0.1",
    "lodash.kebabcase": "^4.1.1",
    "mdast-util-to-string": "^4.0.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.15.2",
    "sharp": "^0.34.3",
    "slugify": "^1.6.6",
    "tailwindcss": "^4.1.11",
    "turndown": "^7.2.0",
    "turndown-plugin-gfm": "^1.0.2",
    "uuid": "^11.1.0",
    "xml2js": "^0.6.2",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@pagefind/default-ui": "^1.3.0",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/bun": "^1.2.19",
    "@types/dotenv": "^8.2.3",
    "@types/lodash.kebabcase": "^4.1.9",
    "@types/turndown": "^5.0.5",
    "@types/uuid": "^10.0.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "chrome-launcher": "^1.2.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.0",
    "lint-staged": "^16.1.2",
    "pagefind": "^1.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.11",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  }
}
